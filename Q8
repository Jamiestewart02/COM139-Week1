class Main {
  public static void main(String[] args) {
    int num1 = Console.getInt("Input the first number: ");
    int num2 = Console.getInt("Input the second number: ");
    int num3 = Console.getInt("Input the third number: ");
    int sum = num1 + num2;
    if(sum == num3) {
        System.out.println("The result is: true");
    } else {
        System.out.println("The result is: false");
    }


  }
}
